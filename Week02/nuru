public class nuru {
    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        double number = scn.nextDouble();
        if (number > 0){
            System.out.println("Positive");
        }
        else if (number < 0){
            System.out.println("Negative");
        }
        if (number == 0){
            System.out.println("Zero");
        }
    
    }
   
}